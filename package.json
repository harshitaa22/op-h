  "scripts": {
    "env:staging": "node set-environment.js staging",
    "env:prod": "node set-environment.js production",
    "preinstall": "yarn --cwd ./packages/blocks/core",
    "android": "yarn workspace mobile android --variant=StagingDebug",
    "androidProd": "yarn workspace mobile android --variant=ProductionDebug",
    "compile": "tsc -b --incremental",
    "ios": "yarn workspace mobile  ios",
    "studio": "yarn workspace mobile studio",
    "xcode": "yarn workspace mobile xcode",
    "iosIpad": "yarn workspace mobile ios --simulator='iPad Pro (11-inch) (4th generation)'",
    "iosSE": "yarn workspace mobile ios --simulator='iPhone SE (3rd generation)'",
    "podInstall": "cd packages/mobile/ios && pod install",
    "images": "cp node-runners/RCTUIImageViewAnimated.m node_modules/react-native/Libraries/Image/RCTUIImageViewAnimated.m",
    "createApkStaging": "cd packages/mobile/android && ./gradlew clean && ./gradlew assembleStagingRelease && cd app/build/outputs/apk/release && open . && cd ../../../",
    "cleanGradle": "cd packages/mobile/android && ./gradlew clean && cd ../../../",
    "createAAB": "cd packages/mobile/android && ./gradlew clean && ./gradlew bundleRelease && cd app/build/outputs/bundle/release && open . && cd ../../../",
    "reset-ios": "yarn install && cd packages/mobile/ios && pod install && cd ../../../ && yarn ios",
    "android-mainjs": "npx react-native bundle --platform android --dev false --entry-file packages/mobile/index.js --bundle-output packages/mobile/android/app/src/main/assets/index.android.bundle --assets-dest packages/mobile/android/app/src/main/res",
    "cleanNode": "rm -r node_modules && rm yarn.lock && cd packages/blocks/core && rm -r node_modules && cd ../../../ && cd packages/mobile && rm -r node_modules && cd ../../ && cd packages/framework && rm -r node_modules && cd ../../",
    "postinstall": "patch-package"
  },
